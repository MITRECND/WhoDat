    "aliases": {
    },
    "settings" : {
        "number_of_shards" : 2,
        "number_of_replicas": 1,
        "refresh_interval": "30s",
        "analysis": {
            "normalizer":{
                "lowercase_fold":{
                    "type": "custom",
                    "filter": ["asciifolding", "lowercase"]
                }
            },
            "analyzer": {
                "default": {
                    "type": "custom",
                    "tokenizer": "keyword",
                    "filter": ["asciifolding", "lowercase"]
                },
                "standard_folding":{
                    "type": "custom",
                    "tokenizer": "standard",
                    "filter": ["standard", "asciifolding", "lowercase", "stop"]
                },
                "email_analyzer":{
                    "type": "custom",
                    "tokenizer": "uax_url_email",
                    "filter": ["lowercase", "email", "domain", "unique"]
                },
                "email_domain_analyzer":{
                    "type": "custom",
                    "tokenizer": "uax_url_email",
                    "filter": ["lowercase", "email_domain", "email", "unique"]
                },
                "email_local_analyzer":{
                    "type": "custom",
                    "tokenizer": "uax_url_email",
                    "filter": ["lowercase", "email_local", "email", "unique"]
                },
                "reversed":{
                    "type": "custom",
                    "tokenizer": "keyword",
                    "filter": ["lowercase", "reverse"]
                },
                "domain_analyzer": {
                    "type": "custom",
                    "tokenizer": "keyword",
                    "filter": ["lowercase", "domain", "unique"]
                },
                "array_analyzer": {
                    "type": "custom",
                    "tokenizer": "array_tokenizer",
                    "filter": ["lowercase", "unique"]
                },
                "name_server_analyzer": {
                    "type": "custom",
                    "tokenizer": "array_tokenizer",
                    "filter": ["lowercase", "name_server", "unique"]
                },
                "all_analyzer": {
                    "type": "custom",
                    "tokenizer": "standard",
                    "filter": ["lowercase", "all_filter", "asciifolding", "unique"]
                }
            },
            "tokenizer": {
                "array_tokenizer": {
                    "type": "pattern",
                    "pattern": "[|]"
                }
            },
            "filter": {
                "domain" : {
                   "type" : "pattern_capture",
                   "preserve_original" : "true",
                   "patterns" : [
                      "([\\w]+)",
                      "([\\w\\-]+)",
                      "(\\p{L}+)",
                      "(\\d+)"
                   ]
                },
                "name_server" : {
                   "type" : "pattern_capture",
                   "preserve_original" : "true",
                   "patterns" : [
                      "([\\w]+)",
                      "([\\w\\-]+)",
                      "(\\p{L}+)",
                      "(\\d+)",
                      "(?=((?<![\\w\\-])[\\w\\-]+[.][\\w\\-]+))"
                   ]
                },
                "email" : {
                   "type" : "pattern_capture",
                   "preserve_original" : "true",
                   "patterns" : [
                      "(\\w+)",
                      "(\\p{L}+)",
                      "(\\d+)",
                      "@(.+)"
                   ]
                },
                "email_local" : {
                   "type" : "pattern_capture",
                   "preserve_original" : "false",
                   "patterns" : [
                      "(.+)@"
                   ]
                },
                "email_domain" : {
                   "type" : "pattern_capture",
                   "preserve_original" : "false",
                   "patterns" : [
                      "@(.+)"
                   ]
                },
                "all_filter" : {
                   "type" : "pattern_capture",
                   "preserve_original": "true",
                   "patterns" : [
                      "(\\w+)",
                      "([\\w\\-]+)",
                      "(\\p{L}+)",
                      "(\\d+)"
                   ]
                }
            }
        }
    },
